{
  "name": "agile-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service serve",
    "serve:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service serve",
    "build:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service build",
    "build:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service build",
    "lint:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service lint",
    "lint:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service lint",
    "inspect:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service inspect --mode production > webpack.entity.admin.js",
    "inspect:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service inspect --mode production > webpack.entity.mobile.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "axios-progress-bar": "^1.2.0",
    "axios-retry": "^3.1.2",
    "core-js": "^3.4.1",
    "element-ui": "^2.12.0",
    "perfect-scrollbar": "^1.4.0",
    "vue": "^2.6.10",
    "vue-cropperjs": "^4.0.1",
    "vue-router": "^3.1.3",
    "vue-router-back-button": "^1.2.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.0",
    "@vue/cli-plugin-vuex": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/eslint-config-standard": "^5.0.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "mocker-api": "^1.9.0",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
