{
  "name": "agile-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service serve",
    "serve:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service serve",
    "build:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service build",
    "build:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service build",
    "inspect:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.admin.config.js vue-cli-service inspect --mode production > webpack.entity.admin.js",
    "inspect:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entity.mobile.config.js vue-cli-service inspect --mode production > webpack.entity.mobile.js"
  },
  "dependencies": {
    "axios": "^0.19.1",
    "axios-progress-bar": "^1.2.0",
    "axios-retry": "^3.1.2",
    "content-disposition": "^0.5.3",
    "core-js": "^3.6.4",
    "element-ui": "^2.13.0",
    "perfect-scrollbar": "^1.4.0",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.11",
    "vue-context": "^5.0.0",
    "vue-cropperjs": "^4.0.1",
    "vue-router": "^3.1.3",
    "vue-router-back-button": "^1.2.3",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-pwa": "^4.1.2",
    "@vue/cli-plugin-router": "^4.1.2",
    "@vue/cli-plugin-vuex": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "assign-deep": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "mocker-api": "^1.11.2",
    "prettier": "^1.19.1",
    "sass": "^1.24.4",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
