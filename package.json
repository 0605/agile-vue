{
  "name": "agile-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.admin.config.js vue-cli-service build",
    "build:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.mobile.config.js vue-cli-service build",
    "inspect:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.admin.config.js vue-cli-service inspect --mode production > webpack.entry.admin.js",
    "inspect:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.mobile.config.js vue-cli-service inspect --mode production > webpack.entry.mobile.js",
    "serve:admin": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.admin.config.js vue-cli-service serve",
    "serve:mobile": "cross-env VUE_CLI_SERVICE_CONFIG_PATH=$PWD/vue.entry.mobile.config.js vue-cli-service serve"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "axios-extensions": "^3.1.3",
    "axios-progress-bar": "^1.2.0",
    "core-js": "^3.6.5",
    "element-plus": "^1.0.1-alpha.12",
    "perfect-scrollbar": "^1.5.0",
    "qs": "^6.9.4",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.2",
    "vue-context": "^6.0.0",
    "vue-cropperjs": "^4.1.0",
    "vue-router": "^v4.0.0-rc.1",
    "vue-router-back-button": "^1.3.0",
    "vuex": "^v4.0.0-rc.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.8",
    "@vue/cli-plugin-eslint": "^4.5.8",
    "@vue/cli-plugin-pwa": "^4.5.8",
    "@vue/cli-plugin-router": "^4.5.8",
    "@vue/cli-plugin-vuex": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "assign-deep": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mocker-api": "^2.7.0",
    "prettier": "^2.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
